{
    "name": "@nativescript-community/audio",
    "version": "6.4.13",
    "description": "NativeScript plugin to record and play audio.",
    "main": "./index",
    "sideEffects": false,
    "typings": "./index.d.ts",
    "scripts": {
        "build": "npm run tsc",
        "build.all": "npm run build",
        "build.angular": "../../node_modules/.bin/ng-packagr -p ../../src/audio/angular/package.json -c ../../src/audio/angular/tsconfig.json",
        "readme": "../../node_modules/.bin/readme generate -c ../../tools/readme/blueprint.json",
        "tsc": "../../node_modules/.bin/cpy '**/*.d.ts' '../../packages/audio' --parents --cwd=../../src/audio && ../../node_modules/.bin/tsc -skipLibCheck -d",
        "clean": "../../node_modules/.bin/rimraf ./*.d.ts ./*.js ./*.js.map"
    },
    "nativescript": {
        "platforms": {
            "android": "6.0.0",
            "ios": "6.0.0"
        }
    },
    "keywords": [
        "NativeScript",
        "JavaScript",
        "Android",
        "iOS",
        "music",
        "microphone",
        "recorder",
        "audio",
        "nativescript-community",
        "Angular",
        "Vue",
        "Svelte",
        "React",
        "preview|https://raw.githubusercontent.com/nativescript-community/audio/master/images/demo-ios.gif|iOS Demo",
        "preview|https://raw.githubusercontent.com/nativescript-community/audio/master/images/demo-android.gif|Android Demo"
    ],
    "author": {
